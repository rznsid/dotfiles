*color2:  #363CC6
*.color3: #285EF8
*color3:  #285EF8
*.color4: #B247B6
*color4:  #B247B6
*.color5: #5DB4EE
*color5:  #5DB4EE
*.color6: #B3AAD6
*color6:  #B3AAD6
*.color7: #c7eef4
*color7:  #c7eef4
*.color8: #8ba6aa
*color8:  #8ba6aa
*.color9: #EEB066
*color9:  #EEB066
*.color10: #363CC6
*color10:  #363CC6
*.color11: #285EF8
*color11:  #285EF8
*.color12: #B247B6
*color12:  #B247B6
*.color13: #5DB4EE
*color13:  #5DB4EE
*.color14: #B3AAD6
*color14:  #B3AAD6
*.color15: #c7eef4
*color15:  #c7eef4

! Black color that will not be affected by bold highlighting.
*.color66: #02000E
*color66:  #02000E

! Xclock colors.
XClock*foreground: #c7eef4
XClock*background: #02000E
XClock*majorColor:  rgba:c7/ee/f4/ff
XClock*minorColor:  rgba:c7/ee/f4/ff
XClock*hourColor:   rgba:c7/ee/f4/ff
XClock*minuteColor: rgba:c7/ee/f4/ff
XClock*secondColor: rgba:c7/ee/f4/ff

! Set depth to make transparency work.
URxvt*depth: 32
URxvt.font: xft:agave Nerd Font Mono:style=Bold:size=12
URxvt.boldFont: xft:agave Nerd Font Mono:style=Bold:size=12
URxvt.italicFont: xft:agave Nerd Font Mono:style=Italic:size=12
URxvt.boldItalicFont: xft:agave Nerd Font Mono:style=Bold Italic:size=12
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 92x24
URxvt.internalBorder: 0
URxvt.cursorBlink: false
URxvt.cursorUnderline: true
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.iso14755: false

!! Copy Paste & Other Extensions
URxvt.perl-ext-common: selection-to-clipboard,clipboard
URxvt.clipboard.autocopy:   true
URxvt.copyCommand: xclip -i -selection clipboard
URxvt.pasteCommand: xclip -o -selection clipboard
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
